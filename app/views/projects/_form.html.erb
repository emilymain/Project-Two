<%=form_for @project do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :content %>
    <%= f.text_area :content %>
  </div>
  <div>
    <%= f.label :funding %>
    <%= f.check_box :funding %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<%end%>
